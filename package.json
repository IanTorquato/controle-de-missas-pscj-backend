	"dependencies": {
		"bcrypt": "^5.0.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"knex": "^0.21.1",
		"multer": "^1.4.2",
		"pg": "^8.3.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.6",
		"@types/multer": "^1.4.3",
		"ts-node-dev": "^1.0.0-pre.49",
		"typescript": "^3.9.5"
	},
	"license": "ISC",
	"main": "index.js",
	"name": "server",
	"scripts": {
		"build": "tsc",
		"dev": "tsnd --transpile-only --ignore-watch node_modules src/server.ts",
		"knex:migrate": "knex --knexfile dist/knexfile.js migrate:latest",
		"knex:rollback": "knex --knexfile dist/knexfile.js migrate:rollback",
		"knex:seed": "knex --knexfile dist/knexfile.js seed:run",
		"start": "node dist/src/server.js"
	},
	"version": "1.0.0"
